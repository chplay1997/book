  "scripts": {
    "start": "cross-env NODE_ENV=development DIR=next node scripts/start.js",
    "dev": "nr start",
    "build": "cross-env NODE_ENV=production DIR=next node scripts/build.js",
    "build-debug": "cross-env NODE_ENV=production DIR=next GENERATE_SOURCEMAP=true node scripts/build.js --profile",
    "lint": "cross-env NODE_ENV=production eslint --ext .jsx,.js,.ts,.tsx src/next",
    "lint-fix": "cross-env NODE_ENV=production eslint --ext .jsx,.js,.ts,.tsx src/next --fix",
    "test": "node scripts/test.js",
    "uitest": "cross-env NODE_ENV=development jest --verbose --runInBand --detectOpenHandles --forceExit",
    "trans": "node scripts/trans.js",
    "test:ci": "cross-env CI=true node scripts/test.js -u --verbose --runInBand --detectOpenHandles --forceExit",
    "typedoc": "typedoc ./src/next --ignoreCompilerErrors",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "check-trans": "node scripts/check-trans-tool/check-trans.js",
    "prepare": "husky install",
    "socket-test": "node scripts/load-test.js"
  },
  "browserslist": {
    "production": [
      "> 1%"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": true
        }
      ],
      [
        "import",
        {
          "libraryName": "lodash",
          "libraryDirectory": "",
          "camel2DashComponentName": false
